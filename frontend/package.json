    "lint:check": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "test:coverage": "vitest run --coverage",
    "test:open": "vitest --open",
    "test:watch": "vitest",
    "test": "vitest run",
    "protoc": "npx protoc --ts_out src/stub --proto_path ../proto/Spectator.Protos enums.proto session.proto"
  },
