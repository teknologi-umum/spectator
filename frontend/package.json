{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "start": "vite preview",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc --skipLibCheck && vite build",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "lint:check": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "test:coverage": "vitest run --coverage",
    "test:open": "vitest --open",
    "test:watch": "vitest",
    "test": "vitest run",
    "protoc": "npx protoc --ts_out src/stub --proto_path ../proto/Spectator.Protos enums.proto session.proto input.proto"
  },
